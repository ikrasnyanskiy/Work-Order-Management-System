@model IEnumerable<WorkOrderManagementSystem.Controllers.InvoiceWithDetails>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IsPaid)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorkOrderId)
        </th>
        <th>
        @Html.DisplayNameFor(model => model.BicycleDescription)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BicycleModelName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BicycleModelManufacturerName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CreationTime)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Total)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IsPaid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WorkOrderId)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.BicycleDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BicycleModelName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BicycleModelManufacturerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationTime)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Total)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }

</table>


