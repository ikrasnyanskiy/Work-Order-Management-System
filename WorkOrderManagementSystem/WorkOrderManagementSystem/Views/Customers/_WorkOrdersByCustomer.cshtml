@model IEnumerable<WorkOrderManagementSystem.Controllers.WorkOrderDetails>

<table class="table">
  <tr>
    <th>
      High Priority
    </th>
    <th>
   Status
   </th>
    <th>
     Notes
   </th>
   <th>
     Bicycle Id
        </th>
        <th>
    Bicycle Description
        </th>
        <th></th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
        <td>
         @Html.DisplayFor(modelItem => item.HighPriority)
       </td>
       <td>
         @Html.DisplayFor(modelItem => item.IsCompleted)
        </td>
        <td>
        @Html.DisplayFor(modelItem => item.Notes)
       </td>
        <td>
         @Html.DisplayFor(modelItem => item.BicycleId)
        </td>
        <td>
          @Html.DisplayFor(modelItem => item.BicycleDescription)
        </td>
        <td>
            @*Using relative links to action link to work order edit and work order details for each line*@
          @Html.ActionLink("Edit Work Order", "../WorkOrders/Edit/", new { id = item.Id }) |
          @Html.ActionLink("Details About Work Order", "../WorkOrders/Details/", new { id = item.Id })
    </td>
</tr>
        }
</table>
